      </ImageView>
      <HBox alignment="CENTER">
         <children>
            <Label text="Autor: " />
            <Label fx:id="nameLabel" onMouseReleased="#onOpenGithubAction" styleClass="name-label" text=" Meinil" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="326.0">
         <children>
            <Label text="Version: " />
            <Label fx:id="nameLabel1" text="0.0.1" />
         </children>
      </HBox>
   </children>
   <styleClass>
      <String fx:value="main" />
      <String fx:value="menu-about" />
   </styleClass>
</VBox>
