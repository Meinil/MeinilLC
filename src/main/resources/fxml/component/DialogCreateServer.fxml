<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="200.0" prefWidth="400.0" spacing="10.0" styleClass="main" stylesheets="@../../css/Main.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.meinil.mc.serverlc.controller.DialogCreateServerController">
   <children>
      <Text fx:id="tipsText" strokeType="OUTSIDE" strokeWidth="0.0" />
      <HBox alignment="CENTER" prefWidth="200.0" spacing="10.0">
         <children>
            <Label text="服务器名称: " />
            <TextField fx:id="serverName" promptText="请不要填写中文" />
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Button fx:id="startBtn" disable="true" mnemonicParsing="false" onMouseReleased="#onStartAction" prefWidth="100.0" styleClass="mx-button" text="开启" />
            <Button fx:id="cancelBtn" mnemonicParsing="false" onMouseReleased="#oncCancelAction" prefWidth="100.0" styleClass="mx-button-danger" text="取消" />
         </children>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
