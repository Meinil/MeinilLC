<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="200.0" prefWidth="400.0" spacing="10.0" stylesheets="@../../css/Main.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.meinil.mc.serverlc.controller.DialogMessageController">
   <children>
      <Text fx:id="messageText" strokeType="OUTSIDE" strokeWidth="0.0" text="请关掉所有服务器后重试" textAlignment="CENTER" wrappingWidth="300.0" />
      <HBox alignment="CENTER" prefHeight="58.0" prefWidth="400.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onMouseReleased="#onConfirmAction" prefWidth="80.0" styleClass="mx-button" text="确定" />
            <Button fx:id="closeBtn" mnemonicParsing="false" onMouseReleased="#onCancelAction" prefWidth="80.0" styleClass="mx-button-danger" text="取消" />
         </children>
      </HBox>
   </children>
   <styleClass>
      <String fx:value="main" />
      <String fx:value="dialog-message" />
   </styleClass>
</VBox>
