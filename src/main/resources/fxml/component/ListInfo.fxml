<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="700.0" spacing="20.0" stylesheets="@../../css/Main.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.meinil.mc.serverlc.controller.ListInfoController">
   <children>
      <Label fx:id="settingLabel" onMouseReleased="#onOpenSettingAction" prefWidth="60.0" text=" 设置">
         <graphic>
            <Region prefHeight="200.0" prefWidth="200.0" />
         </graphic>
         <styleClass>
            <String fx:value="icon-btn" />
            <String fx:value="list-setting" />
         </styleClass>
      </Label>
      <Label fx:id="statusLabel" onMouseReleased="#onSetStatusAction" prefWidth="60.0" text=" 启动">
         <graphic>
            <Region prefHeight="200.0" prefWidth="200.0" />
         </graphic>
         <styleClass>
            <String fx:value="icon-btn" />
            <String fx:value="list-status" />
         </styleClass>
      </Label>
      <Label fx:id="removeLabel" onMouseReleased="#onRemoveAction" prefWidth="60.0" text=" 移除">
         <graphic>
            <Region prefHeight="200.0" prefWidth="200.0" />
         </graphic>
         <styleClass>
            <String fx:value="icon-btn" />
            <String fx:value="list-remove" />
         </styleClass>
      </Label>
      <Label fx:id="coreLabel" prefWidth="140.0" text=" 核心">
         <graphic>
            <Region prefHeight="200.0" prefWidth="200.0" />
         </graphic>
         <styleClass>
            <String fx:value="icon-btn" />
            <String fx:value="list-core" />
         </styleClass>
      </Label>
      <Label fx:id="nameLabel" text=" 名字">
         <graphic>
            <Region prefHeight="200.0" prefWidth="200.0" />
         </graphic>
         <styleClass>
            <String fx:value="icon-btn" />
            <String fx:value="list-name" />
         </styleClass>
      </Label>
   </children>
   <padding>
      <Insets left="20.0" />
   </padding>
   <styleClass>
      <String fx:value="main" />
      <String fx:value="list-info" />
   </styleClass>
</HBox>
