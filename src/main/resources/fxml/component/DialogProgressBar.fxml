<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="150.0" prefWidth="500.0" spacing="10.0" stylesheets="@../../css/Main.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.meinil.mc.serverlc.controller.DialogProgressBarController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="正在下载..." />
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <ProgressBar fx:id="progressBar" prefHeight="10.0" prefWidth="200.0" progress="0.0" />
            <Label fx:id="percentageLabel" prefWidth="60.0" styleClass="icon-btn" text="0.0%" />
         </children>
         <padding>
            <Insets left="60.0" />
         </padding>
      </HBox>
      <Button fx:id="closeBtn" mnemonicParsing="false" onMouseReleased="#onStageCloseAction" styleClass="mx-button" text="取消" />
   </children>
   <styleClass>
      <String fx:value="main" />
      <String fx:value="mx-dialog" />
   </styleClass>
</VBox>
